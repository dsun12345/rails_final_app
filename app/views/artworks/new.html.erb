<%= form_for @artwork do |f| %>

<%= render partial: "/partials/errors", locals: {obj: @artwork} %>

    <%= f.label :title %>
    <%= f.text_field :title %>

    <%= f.label :artist %>
    <%= f.text_field :artist %>

    <%= f.collection_select(:museum_id, Museum.all, :id, :name) %>

    <%= f.submit "Create Museum!!!!" %>

<% end %>